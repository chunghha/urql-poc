    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.14",
    "@graphql-codegen/typescript-operations": "^2.3.4",
    "@graphql-codegen/typescript-urql": "^3.5.5",
    "@graphql-codegen/typescript-urql-graphcache": "^2.2.8",
    "@graphql-codegen/typescript-vue-urql": "^2.2.8",
